            mywindowACR = window.open("http://www.localpro.loc/Path/Loadingpage/ACR/ACRLoding.html", '_blank', 'height=13000,width=13000', 'resizable=0', 'close=0');
            setTimeout(function () { mywindow.close(); }, 70000);//add var un time 12:34:47
        }

        
